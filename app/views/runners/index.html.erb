<table>
    <thead>
        <th>First Name</th>
		<th>Last Name</th>
		<th>Skill</th>
		<th>Time available from</th>
		<th>Time available to</th>
		
    </thead>
    <tbody>
<% @runners.each do |runner| %>
    <tr>
        <td><%= runner.fname %></td>
		<td><%= runner.lname %></td>
		<td><%= runner.skill %></td>
		<td><%= runner.time_availfrom %></td>
		<td><%= runner.time_availto %></td>
		<td><a href="/runners/<%= runner.id %>/delete">Delete</a></td>
    </tr>
<% end %>
    </tbody>
</table>


